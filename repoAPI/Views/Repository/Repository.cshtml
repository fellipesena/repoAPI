@model List<Repository>
@{
    ViewData["Title"] = "Pesquisa - ";
}
@section Styles {
    <link href="@Url.Content("~/css/repository.css")" rel="stylesheet" type="text/css" />
}
<div class="text-center">
    @if (Model == null)
    {
        <span>sim piranha, ele é nulo</span>
    }
    else
    {
        if (Model.Count() == 0)
        {
            <span>Não foram encontrados repositórios com este nome.</span>
        }
        else
        {
            foreach (var repository in Model)
            {
                <br>@repository.Name
            }
        }
    }
</div>